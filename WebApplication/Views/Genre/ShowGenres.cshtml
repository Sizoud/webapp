@model List<WebApplication.Models.GameModels.Genre>

@{
    ViewBag.Title = "title";
}
@if (Model.Count > 0)
{
    <nav class="" style="width: 200px; margin-top: 15px; margin-right: 150px">

        @foreach (var item in Model)
        {
            @Html.ActionLink(item.Title, "Genre", "Game", new {id = item.Id}, new {style = "background-color: #fff; color: black"})
        }

    </nav>
}